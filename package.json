{
  "name": "uroborosql-fmt",
  "description": "uroborosql-fmt extension",
  "author": "k-kawabt",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.nasa.future.co.jp/oss-incubate/rust-sql-formatter/uroborosql-fmt-extension"
  },
  "publisher": "k-kawabt",
  "icon": "uroborosql-fmt-icon.png",
  "categories": [
    "Formatters"
  ],
  "keywords": [],
  "engines": {
    "vscode": "^1.63.0"
  },
  "activationEvents": [
    "onLanguage:sql",
    "onCommand:uroborosql-fmt.uroborosql-format"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "commands": [
      {
        "command": "uroborosql-fmt.uroborosql-format",
        "title": "format sql"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src ./server/src --ext .ts,.tsx",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "sh ./scripts/e2e.sh"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "mocha": "^9.2.1",
    "typescript": "^4.8.4"
  }
}
